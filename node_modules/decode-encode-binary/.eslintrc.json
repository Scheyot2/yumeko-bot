{
    "env": {
        "node": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
        "no-inner-declarations": 0,
        "no-console": "off",
        "no-inline-comments": "off"
    }
}
